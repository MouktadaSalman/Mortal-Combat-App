<Window x:Class="MortalCombatClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MortalCombatClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="900" Background="Orange" >
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="images/background.jpeg" Stretch="UniformToFill"/>
        </Grid.Background>


        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden">
            <Frame.Background>
                <ImageBrush/>
            </Frame.Background>
        </Frame>
    </Grid>
</Window>
